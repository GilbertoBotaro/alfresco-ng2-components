{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "runtime-app-definition.json",
  "title":"Runtime app definition details",
  "description":"Runtime app definition details schema",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items":{
        "$ref":"app-def.json"
      }
    },
    "size": {
      "type": "integer",
      "title": "The Size Schema ",
      "pattern": "[0-9]+"
    },
    "start": {
      "type": "integer",
      "title": "The Start Schema ",
      "pattern": "[0-9]+"
    },
    "total": {
      "type": "integer",
      "title": "The Total Schema ",
      "pattern": "[0-9]+"
    }
  },
  "additionalProperties": false
}