{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "repository-deployment.json",
  "description":"Repository deployment schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[0-9]+"
    },
    "name": {
      "type": "string"
    },
    "deploymentTime": {
      "type": "string",
      "format": "date-time"
    },
    "category": {
      "type": "null"
    },
    "tenantId": {
      "type": "string",
      "pattern": "^(tenant_)?[0-9]+"
    },
    "url": {
      "type": "string",
      "title": "deployment url"
    }
  },
  "additionalProperties": false
}
