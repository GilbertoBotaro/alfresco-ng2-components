{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "report.json",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "title": "The Id Schema ",
      "minimum": 1
    },
    "name": {
      "type": "string",
      "title": "The Name Schema ",
      "pattern":"[a-zA-Z0-9]+"
    },
    "created": {
      "type": "string",
      "title": "The Created Schema ",
      "$ref": "date.json"
    },
    "definition": {
      "type": "string",
      "title": "The Definition Schema ",
      "minLength": 1
    }
  },
  "additionalProperties": false
}