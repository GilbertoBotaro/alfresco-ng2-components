{
    "plugins": [
        "jasmine",
        "protractor"
    ],
    "env": {
        "jasmine": true
    },
    "extends": "plugin:jasmine/recommended",
    "globals": {
        "$": true,
        "node": true,
        "jQuery": true,
        "mongoose": true
    },
    "rules": {
        "comma-dangle": 1,
        "no-cond-assign": 1,
        "no-console": 1,
        "no-constant-condition": 1,
        "no-control-regex": 1,
        "no-debugger": 1,
        "no-dupe-args": 1,
        "no-dupe-keys": 1,
        "no-duplicate-case": 1,
        "no-empty-character-class": 1,
        "no-empty": 1,
        "no-ex-assign": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-semi": 2,
        "no-func-assign": 1,
        "no-inner-declarations": 1,
        "no-invalid-regexp": 1,
        "no-irregular-whitespace": 1,
        "no-negated-in-lhs": 1,
        "no-obj-calls": 1,
        "no-regex-spaces": 1,
        "no-sparse-arrays": 1,
        "no-unreachable": 1,
        "use-isnan": 1,
        "valid-typeof": 1,
        "no-fallthrough": 1,
        "no-octal": 1,
        "no-redeclare": 1,
        "no-delete-var": 1,
        "no-undef": 2,
        "no-unused-vars": 1,
        "no-mixed-spaces-and-tabs": 1,
        "eqeqeq": 2,
        "one-var": [2, "never"],
        "consistent-return": 2,
        "indent": [1, 4],
        "no-else-return": 1,
        "semi": [1, "always"],
        "space-unary-ops": 2,
        //"quotes": [2, "single"]
        "jasmine/missing-expect": 2,
        "jasmine/no-spec-dupes": [1, "block"],
        "jasmine/no-suite-dupes": [1, "block"],
        "jasmine/no-describe-variables": 0
    }
}