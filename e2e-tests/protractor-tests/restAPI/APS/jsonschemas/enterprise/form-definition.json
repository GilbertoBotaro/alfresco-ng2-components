{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "form-definition.json",
  "type": "object",
  "description": "Form definition description",
  "properties": {
    "className": {
      "type": ["string", "null"]
    },
    "customFieldTemplates": {
      "type": "object"
    },
    "customFieldsValueInfo": {
      "type": "object"
    },
    "fields": {
      "type": "array",
      "items": {
        "$ref": "form-field.json"
      }
    },
    "globalDateFormat": {
      "type": "string",
      "title": "The Globaldateformat Schema ",
      "minLength": 1
    },
    "gridsterForm": {
      "type": "boolean",
      "title": "The Gridsterform Schema "
    },
    "id": {
      "type": "integer",
      "title": "The Id Schema ",
      "pattern":"[0-9]",
      "minimum":1
    },
    "javascriptEvents": {
      "type": "array",
      "items": {
        "$ref": "form-javascript-event.json"
      }
    },
    "metadata": {
      "type": "object"
    },
    "name": {
      "type": "string",
      "title": "The Name Schema ",
      "minLength": 1,
      "pattern":"^[a-zA-Z]"
    },
    "outcomeTarget": {
      "type": "string",
      "title": "The Outcometarget Schema "
    },
    "outcomes": {
      "type": "array",
      "items": {
        "$ref": "form-outcome.json"
      }
    },
    "processDefinitionId": {
      "type": "string",
      "title": "The Processdefinitionid Schema ",
      "minLength": 1,
      "pattern":"^[a-zA-Z0-9]"
    },
    "processDefinitionKey": {
      "type": "string",
      "title": "The Processdefinitionkey Schema ",
      "minLength": 1,
      "pattern":"^[a-zA-Z]"
    },
    "processDefinitionName": {
      "type": "string",
      "title": "The Processdefinitionname Schema "
    },
    "selectedOutcome": {
      "type": "string",
      "title": "The Selectedoutcome Schema "
    },
    "style": {
      "type": "string",
      "title": "The Style Schema "
    },
    "tabs": {
      "type": "array",
      "items": {
        "$ref": "form-tab.json"
      }
    },
    "taskDefinitionKey": {
      "type": "string",
      "title": "The Taskdefinitionkey Schema ",
      "minLength": 1,
      "pattern": "^[a-zA-Z0-9]+"
    },
    "taskId": {
      "type": "string",
      "title": "The Taskid Schema ",
      "pattern": "[0-9]",
      "minLength": 1
    },
    "taskName": {
      "type": "string",
      "title": "The Taskname Schema ",
      "minLength": 1,
      "pattern": "^[a-zA-Z]+"
    },
    "variables": {
      "type": "array",
      "items": {
        "$ref": "variable.json"
      }
    }
  },
  "additionalProperties":false
}