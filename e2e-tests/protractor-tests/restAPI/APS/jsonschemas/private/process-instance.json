{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "process-instance.json",
  "type": "object",
  "title":"Process instance",
  "description":"Process instance schema",
  "properties": {
    "id": {
      "type": "string",
      "title": "The Id Schema ",
      "pattern": "[0-9]+"
    },
    "name": {
      "type": "string",
      "title": "The Name Schema ",
      "pattern": "[a-zA-z0-9]+"
    },
    "businessKey": {
      "type": ["string", "null"],
      "title": "The Businesskey Schema "
    },
    "processDefinitionId": {
      "type": "string",
      "title": "The Processdefinitionid Schema ",
      "pattern": "[a-zA-z0-9]+"
    },
    "tenantId": {
      "type": "string",
      "title": "The Tenantid Schema ",
      "pattern": "[a-zA-z0-9]+"
    },
    "started": {
      "type": "string",
      "title": "The Started Schema ",
      "$ref": "date.json"
    },
    "ended": {
      "title": "The Ended Schema ",
      "oneOf": [
        {"$ref": "date.json"},
        {"type": "null"}
      ]
    },
    "startedBy": {
      "$ref": "lightUser.json"
    },
    "processDefinitionName": {
      "type": "string",
      "title": "The Processdefinitionname Schema ",
      "pattern": "[a-zA-z]+"
    },
    "processDefinitionDescription": {
      "type": ["string", "null"],
      "title": "The Processdefinitiondescription Schema ",
      "pattern": "[a-zA-z0-9]+"
    },
    "processDefinitionKey": {
      "type": "string",
      "title": "The Processdefinitionkey Schema ",
      "pattern": "[a-zA-z]+"
    },
    "processDefinitionCategory": {
      "type": "string",
      "title": "The Processdefinitioncategory Schema ",
      "pattern": "[a-zA-z]+"
    },
    "processDefinitionVersion": {
      "type": "integer",
      "title": "The Processdefinitionversion Schema ",
      "minimum": 1
    },
    "processDefinitionDeploymentId": {
      "type": "string",
      "title": "The Processdefinitiondeploymentid Schema ",
      "pattern": "[0-9]+"
    },
    "graphicalNotationDefined": {
      "type": "boolean",
      "title": "The Graphicalnotationdefined Schema "
    },
    "startFormDefined": {
      "type": "boolean",
      "title": "The Startformdefined Schema "
    },
    "suspended": {
      "type": "boolean",
      "title": "The Suspended Schema "
    },
    "variables": {
      "type": "array",
      "items": {
        "$ref": "variable.json"
      }
    }
  },
  "additionalProperties": false
}